CREATE OR REPLACE PACKAGE HR.PKG_DEMO2 AS
    FUNCTION SUMA(N1 IN NUMBER, N2 IN NUMBER) RETURN NUMBER;
    FUNCTION SUMA(N1 IN NUMBER, N2 IN NUMBER, N3 IN NUMBER) RETURN NUMBER;
END PKG_DEMO2;
/
--Cuerpo del paquete
CREATE OR REPLACE PACKAGE BODY HR.PKG_DEMO2 AS
    FUNCTION SUMA(N1 IN NUMBER, N2 IN NUMBER) RETURN NUMBER AS
        RTN NUMBER;
    BEGIN
        RTN := N1 + N2;
        RETURN RTN;
    END;

    FUNCTION SUMA(N1 IN NUMBER, N2 IN NUMBER, N3 IN NUMBER) RETURN NUMBER AS
        RTN NUMBER;
    BEGIN
        RTN := N1 + N2 + N3;
        RETURN RTN;
    END;
END PKG_DEMO2;
/
-- Ejemplos de ejecución
SELECT HR.PKG_DEMO2.SUMA(12, 13) FROM DUAL;
SELECT HR.PKG_DEMO2.SUMA(12, 13, 25) FROM DUAL;

SELECT 
    HR.PKG_DEMO2.SUMA(12, 13) AS SUMA1,
    HR.PKG_DEMO2.SUMA(12, 13, 25) AS SUMA2
FROM DUAL;
-- Especificación del paquete
CREATE OR REPLACE PACKAGE HR.PKG_DEMO3 AS
    PROCEDURE SUMA(N1 IN NUMBER, N2 IN NUMBER);
    PROCEDURE SUMA(A IN NUMBER, B IN NUMBER);
END PKG_DEMO3;
/

-- Cuerpo del paquete
CREATE OR REPLACE PACKAGE BODY HR.PKG_DEMO3 AS
    PROCEDURE SUMA(N1 IN NUMBER, N2 IN NUMBER) AS
        N3 NUMBER;
    BEGIN
        N3 := N1 + N2;
        DBMS_OUTPUT.PUT_LINE('N3: ' || N3);
    END;

    PROCEDURE SUMA(A IN NUMBER, B IN NUMBER) AS
        C NUMBER;
    BEGIN
        C := A + B;
        DBMS_OUTPUT.PUT_LINE('C: ' || C);
    END;
END PKG_DEMO3;
/

-- Ejemplos de ejecución

BEGIN
    HR.PKG_DEMO3.SUMA(A => 13, B => 18);  -- Llama a SUMA(A, B)
END;
/


