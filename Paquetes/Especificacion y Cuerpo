--Especificacion 
CREATE OR REPLACE PACKAGE HR.PKG_DEMO AS
    FUNCTION SUMA(N1 IN NUMBER, N2 IN NUMBER) RETURN NUMBER;
END PKG_DEMO;
/
--Cuerpo del paquete
CREATE OR REPLACE PACKAGE BODY HR.PKG_DEMO AS
    FUNCTION SUMA(N1 IN NUMBER, N2 IN NUMBER) RETURN NUMBER IS
        V_RESULTADO NUMBER;
    BEGIN
        V_RESULTADO := N1 + N2;
        RETURN V_RESULTADO;
    END SUMA;
END PKG_DEMO;
/

